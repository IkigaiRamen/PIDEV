<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="441.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.LoginController">
   <children>
      <AnchorPane layoutX="115.0" layoutY="29.0" prefHeight="384.0" prefWidth="433.0" style="-fx-border-color: #7C7C7C;">
         <children>
            <Label layoutX="150.0" layoutY="34.0" text="Connectez-vous">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="email" layoutX="105.0" layoutY="100.0" prefHeight="25.0" prefWidth="223.0" promptText="E-mail" />
            <TextField fx:id="password" accessibleRole="PASSWORD_FIELD" layoutX="105.0" layoutY="157.0" prefHeight="25.0" prefWidth="223.0" promptText="Mot de passe" />
            <Hyperlink fx:id="forgotpw" layoutX="138.0" layoutY="200.0" onAction="#forgotpw" text="Oubliez votre mot de passe?" />
            <Button fx:id="login" layoutX="174.0" layoutY="240.0" mnemonicParsing="false" onAction="#login" text="Se connecter" />
            <Button fx:id="inscription" layoutX="182.0" layoutY="320.0" mnemonicParsing="false" onAction="#inscription" text="Inscription" />
            <Label layoutX="125.0" layoutY="282.0" text="Vous etes nouveau? Inscrivez-vous" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
